{
  "name": "j2se-js",
  "version": "0.0.0-development",
  "description": "j2se-js : a JavaScript porting of j2se framework",
  "main": "src/index.ts",
  "directories": {
    "src": "src",
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "commit": "git-cz",
    "test:watch": "mocha -r ts-node/register **/*.spec.ts --watch-extensions ts --watch",
    "test": "nyc -e .ts -x **/*.spec.ts mocha -r ts-node/register **/*.spec.ts",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "docs": "typedoc --out ./docs ./src --exclude ./test",
    "build": "rimraf dist && tsc --project tsconfig.prod.json && webpack",
    "build-dev": "rimraf dist && tsc --project tsconfig.dev.json",
    "lint": "",
    "postinstall": "",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/apuliasoft/j2se-js.git"
  },
  "keywords": [
    "j2se",
    "java",
    "util"
  ],
  "author": "Carlotta Dimatteo <carlotta.dimatteo@apuliasoft.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/apuliasoft/j2se-js/issues"
  },
  "homepage": "https://github.com/apuliasoft/j2se-js#readme",
  "devDependencies": {
    "@types/chai": "3.4.35",
    "@types/mocha": "2.2.40",
    "chai": "3.5.0",
    "commitizen": "2.9.6",
    "coveralls": "2.13.0",
    "cz-conventional-changelog": "2.0.0",
    "mocha": "3.2.0",
    "mocha-lcov-reporter": "1.3.0",
    "nyc": "10.2.0",
    "rimraf": "2.6.1",
    "semantic-release": "6.3.2",
    "ts-loader": "2.0.3",
    "ts-node": "3.0.2",
    "typedoc": "0.5.10",
    "typescript": "2.2.2",
    "typings": "2.1.0",
    "webpack": "2.3.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
